#include<iostream>
using namespace std;
inline int max(int a,int b,int c)
{
  if(a>b&&a>c)
    return a;
  else if(b>c)
    return b;
  else 
    return c;
}
int main()
{
int a,b,c,largest;
cout<<"enter 3 numbers\n";
cin>>a>>b>>c;
largest=max(a,b,c);
cout<<"maximum of 3 nos:"<<largest;
return 0;
}
       
